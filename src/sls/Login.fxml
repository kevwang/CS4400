<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" prefHeight="300.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sls.LoginController">
   <children>
      <HBox maxWidth="250.0" prefHeight="30.0" prefWidth="250.0">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Username" />
            <Pane prefHeight="200.0" prefWidth="10.0" HBox.hgrow="ALWAYS" />
            <TextField maxWidth="150.0" prefWidth="150.0" />
         </children>
      </HBox>
      <HBox maxWidth="250.0" prefHeight="60.0" prefWidth="250.0">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Password" />
            <Pane prefHeight="200.0" prefWidth="10.0" HBox.hgrow="ALWAYS" />
            <PasswordField maxWidth="150.0" prefWidth="150.0" />
         </children>
      </HBox>
      <Button mnemonicParsing="false" onAction="#loginClicked" text="Login">
         <VBox.margin>
            <Insets bottom="15.0" />
         </VBox.margin></Button>
      <Button mnemonicParsing="false" onAction="#registerClicked" text="Register" />
   </children>
</VBox>
